{
  "general": {
    "preferredEditor": "nvim",
    "vimMode": false,
    "autoUpdate": true,
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "customThemes": {
      "Cobalt2": {
        "name": "Cobalt2",
        "type": "custom",
        "Background": "#193549",
        "Foreground": "#FFFFFF",
        "LightBlue": "#00AAFF",
        "AccentBlue": "#0050a4",
        "AccentPurple": "#FF80E1",
        "AccentCyan": "#80FFBB",
        "AccentGreen": "#3AD900",
        "AccentYellow": "#FFC600",
        "AccentRed": "#ee3a43",
        "Comment": "#0088FF",
        "Gray": "#8a8a8a",
        "DiffAdded": "#3AD900",
        "DiffRemoved": "#ee3a43",
        "DiffModified": "#FF9D00"
      }
    },
    "theme": "Default",
    "hideBanner": true,
    "hideTips": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time"]
    },

    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },

    "git": {
      "args": ["mcp-server-git", "--repository", "${REPOSITORY_PATH}"],
      "command": "uvx",
      "disabled": false
    },

    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },

    "context7": {
      "args": ["-y", "@upstash/context7-mcp"],
      "command": "npx",
      "disabled": false,
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "${DEFAULT_MINIMUM_TOKENS}"
      }
    },

    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    }
  }
}
